{
  "id": "tradle.CustomerIdentification",
  "title": "Customer identification",
  "interfaces": [
    "tradle.Message"
  ],
  "type": "tradle.Model",
  "properties": {
    "_t": {
      "type": "string",
      "readOnly": true
    },
    "from": {
      "type": "object",
      "readOnly": true,
      "ref": "tradle.Identity"
    },
    "to": {
      "type": "object",
      "readOnly": true,
      "ref": "tradle.Identity"
    },
    "contactsIntroducingCompany": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "contact": {
            "type": "object",
            "ref": "tradle.Identity",
            "skipLabel": true
          },
          "email": {
            "type": "string"
          },
          "phone": {
            "type": "string"
          }
        }
      }
    },
    "introducedParentCompany": {
      "type": "object",
      "ref": "tradle.Organization",
      "title": "Introduced Parent Company (legally registered name of entity) "
    },
    "accountName": {
      "type": "string",
      "title": "Account name"
    },
    "officeAddress": {
      "type": "string"
    },
    "countryOfOrigin": {
      "type": "object",
      "ref": "tradle.Country"
    },
    "contactPerson": {
      "type": "object",
      "ref": "tradle.Identity"
    },
    "isYouCompanyListed": {
      "type": "object",
      "ref": "tradle.Organizations"
    },
    "stockExchange": {
      "type": "object",
      "ref": "tradle.StockExchange",
      "description": "Stock exchange at which your company is listed"
    }
  }
}